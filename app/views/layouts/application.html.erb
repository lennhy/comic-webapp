<!DOCTYPE html>
<html>
  <head>
    <title>ComicWebapp</title>
    <%= csrf_meta_tags %>

    <% if user_signed_in? %>
        <%= link_to("Sign Out", destroy_user_session_path, method: :delete) %>
    <% end %>

    <% if !user_signed_in? %>
      <%= link_to("Sign Up", new_user_registration_path) %>
    <% end %>

    <%= stylesheet_link_tag  'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body ng-app="app">
    <!-- <div ng-repeat="comic in vm.comics" >    Hello {{ comic  }} </div> -->
    <%= yield %>
  </body>
</html>
